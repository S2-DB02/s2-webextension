{
    "name": "BAS World bug reporting",
    "description": "Let's report some bugs!",
    "version": "0.1",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/images/icon16.png",
        "32": "/images/icon32.png",
        "48": "/images/icon64.png"
      }
    },
    "permissions": [
      "contextMenus"
    ],
    "icons": {
      "16": "/images/icon16.png",
      "32": "/images/icon32.png",
      "48": "/images/icon64.png"
    }
  }